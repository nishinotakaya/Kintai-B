<h1>利用者一覧</h1>

<table class="table">
  <thead>
    <tr>
      <td></td>
      <td></td>
      <td>利用者名</td>
      <td>連絡先</td>
      <td>介護度</td>
      <td>利用日</td>
    </tr>
  </thead>
  
  
  <tbody>
    <% @clients.each do |client| %>
      <tr>
        <td><%= link_to "利用者情報編集", edit_client_path(client) %></td>
        <td></td>
        <td><%= client.client_name %></td>
        <td><%= client.client_email %></td>
        <td><%= client.nursing_number %></td>
        <td><%= client.date_of_day %></td>
      </tr>
    <% end %>  
  </tbody>
</table>  
        
      